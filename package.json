    "semver": "7.3.7"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/node": "17.0.24",
    "@types/semver": "7.3.9",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "jest": "27.5.1",
    "jest-circus": "27.5.1",
    "npm-run-all": "4.1.5",
